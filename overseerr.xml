<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>overseerr</Name>
  <Repository>sctx/overseerr</Repository>
  <Registry>https://hub.docker.com/r/sctx/overseerr</Registry>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support></Support>
  <Project>https://overseerr.dev/</Project>
  <Overview>Overseerr is a free and open source software application for managing requests for your media library. It integrates with your existing services such as Sonarr, Radarr and Plex!</Overview>
  <Category>Downloaders: MediaApp:Video</Category>
  <WebUI>http://[IP]:[PORT:5055]/</WebUI>
  <TemplateURL></TemplateURL>
  <Icon>https://raw.githubusercontent.com/sct/overseerr/develop/public/os_logo_square.png</Icon>
  <ExtraParams>--restart unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <Description>Overseerr is a free and open source software application for managing requests for your media library. It integrates with your existing services such as Sonarr, Radarr and Plex!</Description>
  <Labels/>
  <Config Name="Persistent Data Storage" Target="/config" Default="/mnt/user/appdata/docker/overseerr/config" Mode="rw" Description="/config" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="HTTP Web Port" Target="5055" Default="5055" Mode="tcp" Description="HTTP Web Port" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Log Level" Target="LOG_LEVEL" Default="info" Description="Logging Level" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="Time Zone" Target="TZ" Default="America/Chicago" Description="Timezone" Type="Variable" Display="always" Required="true" Mask="false"/>

</Container>